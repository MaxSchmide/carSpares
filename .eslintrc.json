{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "next"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["error", "single"],
    "semi": "error",
    "no-unused-vars": "warn",
    "no-console": "error",
    "indent": ["error", 2]
  }
}
